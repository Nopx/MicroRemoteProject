package global.util;

import java.util.Observable;

abstract public class Driver extends Observable{
	
	abstract public void initialize();
	abstract public void dispose();
	
	public Driver(){
		//First message that isnt heard
		omitMessage("");
	}
	
	
	protected void omitMessage(String msg){
		notifyObservers(msg);
		setChanged();
	}
	
    
}